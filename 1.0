local isMinimized = false
local function toggleMinimize()
    isMinimized = not isMinimized
    if isMinimized then
        minimizeButton.Text = MJnagHUdpbMNDfvAyJZOCsnYGPtBjlsFysWByWpoTGsZTCIKJ('NcquvWaQZvhYthxctSoZOfVJEDnqVkBDqedBpohbSgjjRMYycXdxaKAKw==')
        for _, child in pairs(frame:GetChildren()) do
            if child ~= title and child ~= minimizeButton then
                child.Visible = false
            end
        end
        frame.Size = UDim2.new(0, 200, 0, 30) -- Thu nhỏ
    else
        minimizeButton.Text = MJnagHUdpbMNDfvAyJZOCsnYGPtBjlsFysWByWpoTGsZTCIKJ('ZORIcPHxomPtRDsaFZzaDEJSKSAGSVMLzDxngjNJPjivfLDvaCKoYOFLQ==')
        for _, child in pairs(frame:GetChildren()) do
            child.Visible = true
        end
        frame.Size = UDim2.new(0, 200, 0, 250) -- Mở rộng
    end
end

minimizeButton.MouseButton1Click:Connect(toggleMinimize)

