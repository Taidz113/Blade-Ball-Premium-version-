local function toggleMinimize()
    isMinimized = not isMinimized
    if isMinimized then
        minimizeButton.Text = tmysTmJYWQuEfvLEaMomiLMbkgsYgTciEbgBRFFgBJCwHjnNCQnLOebbIKuMdVXfIoKAgyOAeSVybhkcgFv('ByyCUtIndOealHxkGaqNbWyLWRzWcoeaVVmNoktviCghwZiYNbJkqRXKw==')
        for _, child in pairs(frame:GetChildren()) do
            if child ~= title and child ~= minimizeButton then
                child.Visible = false
            end
        end
        frame.Size = UDim2.new(0, 200, 0, 30)
    else
        minimizeButton.Text = tmysTmJYWQuEfvLEaMomiLMbkgsYgTciEbgBRFFgBJCwHjnNCQnLOebbIKuMdVXfIoKAgyOAeSVybhkcgFv('lAiITWlsSazYNsHFeHqSBMCTaiYmokjwvWLdbwWyCuUKjKqrixphwhLLQ==')
        for _, child in pairs(frame:GetChildren()) do
            child.Visible = true
        end
        frame.Size = UDim2.new(0, 200, 0, 250)
    end
end

